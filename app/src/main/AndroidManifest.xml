<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.helloworld">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/NoActionBar">
        <activity android:name="com.example.myapplication.ProtectPasswordActivity" />
        <activity android:name="com.example.myapplication.ForgetPasswordActivity" />
        <activity android:name="com.example.myapplication.SettingActivity" />
        <activity android:name="com.example.myapplication.LoginActivity" />
        <activity android:name="com.example.myapplication.RegisterActivity" />
        <activity android:name="com.example.myapplication.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.myapplication.MainActivity" />
    </application>

</manifest>