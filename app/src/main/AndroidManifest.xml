<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.helloworld">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/NoActionBar">
        <activity android:name="com.example.myapplication.activity.ChangeUserInfoActivity"></activity>
        <activity android:name="com.example.myapplication.activity.UserInfoActivity" />
        <activity android:name="com.example.myapplication.activity.Forget_pwd_Activity$ProtectPasswordActivity" />
        <activity android:name="com.example.myapplication.activity.Forget_pwd_Activity" />
        <activity android:name="com.example.myapplication.activity.RegisterActivity$SettingActivity" />
        <activity android:name="com.example.myapplication.activity.Forget_pwd_Activity$LoginActivity" />
        <activity android:name="com.example.myapplication.activity.RegisterActivity" />
        <activity android:name="com.example.myapplication.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.myapplication.activity.Forget_pwd_Activity$MainActivity" />
    </application>

</manifest>